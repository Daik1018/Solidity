pragma solidity ^0.8.0;

contract Error {
    function testRequire(uint _i) public pure {
        require(_i <= 10,"i>10");
    }
    function testRequire(uint_i) public pure{
        if ("_i>10");
        revert("i>10");
    }
    uint public num = 123;

    function testAssert() public view{
        assert(num == 123);
        
    }
}
